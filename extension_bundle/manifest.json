{
    "manifest_version": 2,
    "name": "Google Sheet Quick Inserts",
    "version": "0.1",
    "permissions": ["activeTab"],
    "description": "A quick solution to adding rows to a google sheet",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}